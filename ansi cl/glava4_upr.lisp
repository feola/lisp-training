;; 1. Определите функцию, поворачивающую квадратный массив (n x n)
;; на 90 градусов по часовой стрелке

(defun quarter-turn (x)
  (make-array (array-dimensions x))
  (setf (aref x 0 0) (aref x 1 0))
  x)

(quarter-turn #2A((a b) (c d)))



